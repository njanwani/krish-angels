<?xml version="1.0"?>

<robot name="threedofexample">

  <!-- ******************************************************** -->
  <!-- ******************* Kinematic Chain ******************** -->
  <!-- ******************************************************** -->
  <!-- Note the 'origin' tag in the joints defines the shift and
       reorientation, i.e. the transform.  For moving joints, this
       happens before the continuous joint rotates around 'axis'. -->

  <!-- World can't take a visual element (at least in Gazebo). -->
  <link name="world"/>

  <joint name="placement" type="fixed">
    <parent link="world"/>
    <child  link="baseplate"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>			
  </joint>

  <!-- BASE PLATE: Shift across the base plate to the base motor. --> 
  <link name="baseplate">
    <visual>
      <origin xyz="0.0585 0.09093 0" rpy="1.5708 0 -1.5708"/>
      <geometry>
        <mesh
          filename="package://basic134/meshes/base.STL" 
          scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="wood"/>
    </visual>
  </link>

  <joint name="baseplate" type="fixed">
    <parent link="baseplate"/>
    <child  link="basemotor"/>
    <origin xyz="-0.0215 0 0.0125" rpy="0 0 0"/>
  </joint>

  <!-- BASE MOTOR: Thickness: X5 = 31.1mm, X8 = 45.1mm -->
  <link name="basemotor">
    <visual>
    <origin xyz="0 0 0" rpy="0 0 1.5708"/>
      <geometry>
        <mesh filename="package://basic134/meshes/X8.stl"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>

  <joint name="base" type="continuous">
    <parent link="basemotor"/>
    <child  link="lbracket"/>
    <origin xyz="0 0 0.0451" rpy="0 0 0"/>
    <axis   xyz="0 0 1"/>
  </joint>

  <!-- L BRACKET: Shift and rotate across the L bracket. -->
  <link name="lbracket">
    <visual>
      <origin xyz="0.04 -0.04 -0.041" rpy="0 0 1.5708"/>
      <geometry>
        <mesh filename="package://basic134/meshes/shoulder.STL"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" /> <!-- Grey color-->
      </material>    
    </visual>
  </link>

  <joint name="lbracket" type="fixed">
    <parent link="lbracket"/>
    <child  link="shouldermotor"/>
    <origin xyz="0 -0.0225 0.0575" rpy="0 0 0"/>	  <!-- L BRACKET DIMENIONS -->
  </joint>

  <!-- SHOULDER MOTOR: Thickness: X5 = 31.1mm, X8 = 45.1mm -->
  <link name="shouldermotor">
    <visual>
    <origin xyz="0 0 0" rpy="-1.5708 1.5708 0"/>
      <geometry>
        <mesh filename="package://basic134/meshes/X8.stl"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>

  <joint name="shoulder" type="continuous">
    <parent link="shouldermotor"/>
    <child  link="upperarm"/>
    <origin xyz="0 0.0516 0" rpy="0 0 0"/>
    <axis   xyz="0 1 0"/>
  </joint>

  <!-- UPPER ARM: Shift across the upper arm (1/4in thickness) -->
  <link name="upperarm">
    <visual>
      <origin xyz="0.477 0 0.0378" rpy="3.1459 0 -1.5708"/>
      <geometry>
        <mesh filename="package://basic134/meshes/first_link.STL"
        scale="0.001 0.001 0.001"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" /> <!-- Grey color-->
      </material> 
    </visual>
    <!-- Add a visual element -->
  </link>

  <joint name="upperarm" type="fixed">
    <parent link="upperarm"/>
    <child  link="elbowmotor"/>
    <origin xyz="0.445 -0.00635 0.004" rpy="0 0 0"/>		  <!-- UPPER ARM DIMENIONS -->
  </joint>

  <!-- Elbow motor.  Thickness: X5 = 31.1mm, X8 = 45.1mm -->
  <link name="elbowmotor">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <mesh filename="package://basic134/meshes/X5.stl"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>

  <joint name="elbow" type="continuous">
    <parent link="elbowmotor"/>
    <child  link="lowerarm"/>
    <origin xyz="0 -0.0376 0" rpy="0 0 0"/>
    <axis   xyz="0 1 0"/>
  </joint>

  <!-- Shift across the upper arm (1/4in thickness) -->
  <link name="lowerarm">
    <visual>
    <origin xyz="0.678 0.007 0.0367" rpy="3.1459 0 -1.5708"/>
      <geometry>
        <mesh filename="package://basic134/meshes/second_link.STL"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" /> <!-- Grey color-->
      </material> 
    </visual>
  </link>

  <joint name="lowerarm" type="fixed">
    <parent link="lowerarm"/>
    <child  link="wristmotor"/>
    <origin xyz="0.647 0.0065 0.0035" rpy="0 0 0"/>		  <!-- LOWER ARM DIMENIONS -->
  </joint>

  <link name="wristmotor">
    <visual>
      <origin xyz="0 0 0" rpy="-1.5708 0 0"/>
      <geometry>
        <mesh filename="package://basic134/meshes/X5.stl"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>

  <joint name="wrist" type="continuous">
    <parent link="wristmotor"/>
    <child  link="endbracket"/>
    <origin xyz="0 0.0516 0" rpy="0 0 0"/>
    <axis   xyz="0 1 0"/>
  </joint>

  <link name="endbracket">  
    <visual>
      <origin xyz="-0.04 -0.0659 -0.04" rpy="-1.5708 -1.5708 0"/>
      <geometry>
        <mesh filename="package://basic134/meshes/wrist.STL"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" /> <!-- Grey color-->
      </material> 
    </visual>
  </link>

  <joint name="endmotor" type="fixed">
    <parent link="endbracket"/>
    <child  link="final"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="final">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <mesh filename="package://basic134/meshes/X5.stl"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>

  <joint name="final2" type="continuous">
    <parent link="final"/>
    <child  link="end"/>
    <origin xyz="0.0 0.041 -0.03" rpy="0 0 0"/>
    <axis   xyz="0 0 1"/>
  </joint>


  <link name="end">
     <visual>
      <origin xyz="0.088 -0.055 -0.01" rpy="-1.5708 0 1.5708"/>
      <geometry>
        <mesh filename="package://basic134/meshes/end_effector_v2.STL"
	      scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="hebi"/>
    </visual>
  </link>


  <!-- TIP FRAME
  <link name="tip">
  </link> -->

  
  <!-- *************************************************** -->
  <!-- *************** Visual Link Elements ************** -->
  <!-- *************************************************** -->
  <!-- Note collada .dae files have better color/texture   -->
  <!-- suppport that STL files.                            -->

  <!-- Colors -->
  <material name="wood">  <color rgba="0.82 0.62 0.04 1"/>  </material>
  <material name="hebi">  <color rgba="0.82 0.06 0.06 1"/>  </material>

</robot>
